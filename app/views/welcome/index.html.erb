<%= form_tag login_path, url: {action: 'create'} do %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>

  <%= label_tag :password %>
  <%= password_field_tag :password %>

  <%= submit_tag 'Log In' %>
<% end %>
<%= link_to "Sign in with Google", google_oauth_path %>
<%= link_to "Register", register_path %>
<%= link_to "About Us", about_path %>
