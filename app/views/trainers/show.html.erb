<p><%= link_to "Invite a new client to signup", trainer_invite_path %></p>

<div id="clients">
  <% @trainer.clients.each do |client| %>
    <div class="client-<%= client.id %>">
      <%= client.first_name %> <%= client.last_name %><br>
      <%= link_to client.email, client.email %>
    </div><br>
  <% end %>
</div>
